<?xml version="1.0"?>
<object name="TutorWebQuiz"
   meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">TutorWeb Quiz</property>
 <property name="description">An online quiz.</property>
 <property name="content_icon">folder_icon.gif</property>
 <property name="content_meta_type">TutorWebQuiz</property>
 <property name="product">TutorWeb</property>
 <property name="factory">addTutorWebQuiz</property>
 <property name="immediate_view">quiz_view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
    <element value="QuizResult"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">quiz_view</property>
 <property name="view_methods">
  <element value="quiz_view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="atct_edit"/>
 <alias from="index.html" to="(dynamic view)"/>
 <alias from="properties" to="base_metadata"/>
 <alias from="sharing" to="folder_localrole_form"/>
 <alias from="view" to="quiz_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/quiz_view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="TutorWeb: Grade Assignments"/>
 </action>
 <action title="Properties" action_id="metadata" category="object"
    condition_expr="" url_expr="string:${object_url}/properties"
    visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="References" action_id="references" category="object"
    condition_expr="object/archetype_tool/has_graphviz"
    url_expr="string:${object_url}/reference_graph" visible="True">
  <permission value="Modify portal content"/>
  <permission value="Review portal content"/>
 </action>
 <action title="Folder Listing" action_id="folderlisting" category="folder"
    condition_expr="object/isPrincipiaFolderish"
    url_expr="string:${folder_url}/view" visible="False">
  <permission value="View"/>
 </action>
 <action title="Results" action_id="results" category="object"
    url_expr="string:${object_url}/quiz_results" visible="True" >
<permission value="TutorWeb: Grade Assignments"/>
 </action>

</object>
